<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>/show/search.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>viewShow</from-outcome>
   <to-view-id>/show/view.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/show/view.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>viewEvent</from-outcome>
   <to-view-id>/event/view.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/event/view.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>buyTicket</from-outcome>
   <to-view-id>/ticket/buyTicket.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/ticket/buyTicket.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>payTicket</from-outcome>
   <to-view-id>/payment/payTicket.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/payment/payTicket.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>viewTicket</from-outcome>
   <to-view-id>/ticket/view.xhtml</to-view-id>
   <redirect>
    <view-param>
     <name>id</name>
     <value>#{ticketController.saleId}</value>
    </view-param>
   </redirect>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>register</from-outcome>
   <to-view-id>/user/register.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/subscription/choose.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>assignedSeat</from-outcome>
   <to-view-id>/subscription/assignedSeat.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>carnet</from-outcome>
   <to-view-id>/subscription/carnet.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>freeSeat</from-outcome>
   <to-view-id>/payment/paySubscription.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/subscription/assignedSeat.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>paySubscription</from-outcome>
   <to-view-id>/payment/paySubscription.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/subscription/carnet.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>paySubscription</from-outcome>
   <to-view-id>/payment/paySubscription.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>listShows</from-outcome>
   <to-view-id>/show/search.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>buySubscription</from-outcome>
   <to-view-id>/subscription/choose.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showSubscriptions</from-outcome>
   <to-view-id>/sale/subscriptions.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>showTickets</from-outcome>
   <to-view-id>/sale/tickets.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
